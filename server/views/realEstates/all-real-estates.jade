extends ../shared/layout
block main-content
    a.btn.btn-default(href="/real-estates/create") Add
    a.btn.btn-default(href="/real-estates/search") Search
    ul
        each val in estates
            li.panel-body
                a(href="/real-estates/#{val.id}" )= val.type
                br
                a(href="/real-estates/#{val.id}" )= val.address